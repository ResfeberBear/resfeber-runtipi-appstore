{
  "schemaVersion": 2,
  "services": [
    {
      "name": "bytestash",
      "image": "ghcr.io/jordan-dalby/bytestash:latest",
      "environment": [
        {
          "key": "BASE_PATH",
          "value": ""
        },
        {
          "key": "JWT_SECRET",
          "value": "your-secret"
        },
        {
          "key": "TOKEN_EXPIRY",
          "value": "24h"
        },
        {
          "key": "ALLOW_NEW_ACCOUNTS",
          "value": "true"
        },
        {
          "key": "DEBUG",
          "value": "true"
        },
        {
          "key": "DISABLE_ACCOUNTS",
          "value": "false"
        },
        {
          "key": "DISABLE_INTERNAL_ACCOUNTS",
          "value": "false"
        },
        {
          "key": "OIDC_ENABLED",
          "value": "false"
        },
        {
          "key": "OIDC_DISPLAY_NAME",
          "value": ""
        },
        {
          "key": "OIDC_ISSUER_URL",
          "value": ""
        },
        {
          "key": "OIDC_CLIENT_ID",
          "value": ""
        },
        {
          "key": "OIDC_CLIENT_SECRET",
          "value": ""
        },
        {
          "key": "OIDC_SCOPES",
          "value": ""
        }
      ],
      "internalPort": 5000,
      "volumes": [
        {
          "hostPath": "/your/snippet/path",
          "containerPath": "/data/snippets",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ]
    }
  ]
}
